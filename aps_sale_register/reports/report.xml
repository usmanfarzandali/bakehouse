<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sale_register_report" model="ir.actions.report">
        <field name="name">Sale Register Report</field>
        <field name="model">sale.register.wizard</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">aps_sale_register.sale_register_template</field>
        <field name="report_file">Sale Register Xlsx</field>
        <field name="binding_model_id" ref="aps_sale_register.model_sale_register_wizard"/>
        <field name="binding_type">report</field>
        <field name="print_report_name">'%s' % (object.date_from)</field>
    </record>

